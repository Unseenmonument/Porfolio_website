<!DOCTYPE html>
<html>
  <head>
    <title>Kerron Streater</title>
    <%= csrf_meta_tags %>

    <!-- Preloaded content -->
<!--	<link rel="preload" href="canvas.js" as="script">
    <link rel="preload" href="application.js" as="script"> -->

    <!-- Icon on Tab -->
    <link rel="icon" href="http://example.com/favicon.png">

    <!-- Chrome, Firefox OS and Opera Theme Color -->
	<meta name="theme-color" content="#6D898C">


    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="body">
    <%= yield %>

    <div class="social-media">    	
    	<div class="go_to_top"><%= image_tag("dblchevron.png", :style => 'height: 60px; width: 60px;', :class => "dblchevron") %></div>
		<div>
		    <div class="col-lg-8 social-1">
			    <div class="social-fix">
			    	<div class="social-block">
				    	<div id="linked" class="social">
			    			<%= image_tag("linkedin.png", :class => "social-pic social-basic", :id => "linked-basic") %>
			    			<%= image_tag("linkedin.png", :class => "social-pic social-hover", :id => "linked-hover") %>
				    	</div>
				    </div>
				    <div class="social-block">	
				    	<div id="git" class="social">
				    		<%= image_tag("github.png", :class => "social-pic social-basic", :id => "git-basic") %>
				    		<%= image_tag("github.png", :class => "social-pic social-hover", :id => "git-hover") %>
				    	</div>
				    </div>
				    <div class="social-block">
				    	<div id="twitter" class="social">
			    			<%= image_tag("twitter.png", :class => "social-pic social-basic", :id => "twitter-basic") %>
			    			<%= image_tag("twitter.png", :class => "social-pic social-hover", :id => "twitter-hover") %>
				    	</div>
				    </div>
				    <div class="social-block">	
				    	<div id="insta" class="social">
			    			<%= image_tag("insta.png", :class => "social-pic social-basic", :id => "insta-basic") %>
			    			<%= image_tag("insta.png", :class => "social-pic social-hover", :id => "insta-hover") %>
				    	</div>
				    </div>
		    	</div>
		    </div>
		</div>
		
	
		<div class="row justify-content-center">
			<div style="color: white; margin-bottom: 20px">KERRON STREATER Â©2020</div>
		</div>
	</div>

	
  </body>
</html>
