<h1>Skills#index</h1>
<table>
  	<thead>
	    <tr>
	      <th>name</th>
	      <th>proficientcy</th>
	      <th>show</th>
	      <th>edit</th>
	      <th>delete</th>
	    </tr>
  	</thead>

  	<tbody>
		<% @skills.each do |skill| %>
		    <tr>
		        <td><%= skill.name %></td>
		        <td><%= skill.proficientcy %></td>

		        <td><%= link_to 'Show', skill %></td>
		        <td><%= link_to 'Edit', edit_skill_path(skill) %></td>
		        <td><%= link_to 'Destroy', skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		    </tr>
		<% end %>
	</tbody>
</table>